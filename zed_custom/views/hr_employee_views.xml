<odoo>
  <record id="view_hr_employee_form_zed" model="ir.ui.view">
    <field name="name">hr.employee.form.zed</field>
    <field name="model">hr.employee</field>
    <field name="inherit_id" ref="hr.view_employee_form"/>
    <field name="arch" type="xml">
      <notebook position="inside">
        <page string="Barista">
          <group>
            <field name="is_barista"/>
            <field name="barista_level"/>
            <field name="certification"/>
          </group>
        </page>
      </notebook>
    </field>
  </record>
</odoo>
